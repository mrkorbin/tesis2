<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="ve.drkorbin.tesis.AdminPrincipalActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cargar Guia"
        android:id="@+id/button14Guia"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="57dp"
        android:layout_alignRight="@+id/textView11"
        android:layout_alignEnd="@+id/textView11"
        android:layout_alignLeft="@+id/button16guia"
        android:layout_alignStart="@+id/button16guia"
        android:onClick="openCargarGuiaActivity" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eliminar Guia"
        android:id="@+id/button15Guia"
        android:layout_below="@+id/button14Guia"
        android:layout_marginTop="52dp"
        android:layout_alignLeft="@+id/button16guia"
        android:layout_alignStart="@+id/button16guia"
        android:layout_alignRight="@+id/button16guia"
        android:layout_alignEnd="@+id/button16guia"
        android:onClick="openEliminarGuiaActivity" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eliminar Usuario"
        android:id="@+id/button16guia"
        android:layout_below="@+id/button15Guia"
        android:layout_alignRight="@+id/textView11"
        android:layout_alignEnd="@+id/textView11"
        android:layout_marginTop="49dp"
        android:onClick="openEliminarUsuarioActivity" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salir"
        android:id="@+id/button17Guia"
        android:layout_below="@+id/button16guia"
        android:layout_marginTop="52dp"
        android:onClick="openLogActivity"
        android:layout_alignRight="@+id/button16guia"
        android:layout_alignEnd="@+id/button16guia"
        android:layout_alignLeft="@+id/button16guia"
        android:layout_alignStart="@+id/button16guia" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Menu de Administrador"
        android:id="@+id/textView11"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
