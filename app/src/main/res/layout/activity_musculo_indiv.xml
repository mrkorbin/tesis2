<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/logoapp"
    tools:context="ve.drkorbin.tesis.MusculoIndivActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Musculo Individual"
        android:id="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bicep"
        android:id="@+id/buttonBicep"
        android:layout_marginTop="46dp"
        android:onClick="openMuscSeleccActivity"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/button23"
        android:layout_toStartOf="@+id/button23" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tricep"
        android:id="@+id/buttonTricep"
        android:layout_alignTop="@+id/buttonBicep"
        android:layout_alignRight="@+id/buttonCuadricep"
        android:layout_alignEnd="@+id/buttonCuadricep"
        android:onClick="openMuscSeleccActivity"
        android:layout_toRightOf="@+id/button23"
        android:layout_toEndOf="@+id/button23" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gemelos"
        android:id="@+id/buttonGemelos"
        android:layout_marginTop="57dp"
        android:layout_below="@+id/buttonBicep"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="openMuscSeleccActivity"
        android:layout_alignRight="@+id/buttonBicep"
        android:layout_alignEnd="@+id/buttonBicep" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cuadricep"
        android:id="@+id/buttonCuadricep"
        android:layout_alignTop="@+id/buttonGemelos"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="openMuscSeleccActivity"
        android:layout_toRightOf="@+id/button23"
        android:layout_toEndOf="@+id/button23" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Femoral"
        android:id="@+id/buttonFemoral"
        android:layout_below="@+id/buttonCuadricep"
        android:layout_marginTop="50dp"
        android:layout_alignParentLeft="true"
        android:onClick="openMuscSeleccActivity"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/buttonGemelos"
        android:layout_alignEnd="@+id/buttonGemelos" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pectoral"
        android:id="@+id/buttonPectoral"
        android:onClick="openMuscSeleccActivity"
        android:layout_alignBottom="@+id/buttonFemoral"
        android:layout_alignRight="@+id/buttonCuadricep"
        android:layout_alignEnd="@+id/buttonCuadricep"
        android:layout_alignLeft="@+id/buttonCuadricep"
        android:layout_alignStart="@+id/buttonCuadricep" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Atras"
        android:id="@+id/button23"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:onClick="openPrincipalActivity" />

</RelativeLayout>
